eact-router-dom'\nimport M from 'materialize-css'\nimport { usercontext } from '../../context'\n\nconst Signin = () => {\n\n\n    const {state, dispatch} = useContext(usercontext)\n    const [username, setUsername] = useState(\"\")\n    const [password, setPassword] = useState(\"\")\n    const history = useHistory()\n\n    const PostData = ()=> {\n        fetch(\"/signin\", {\n            methos: \"POST\",\n            headers: {\n                \"Content-type\":\"application/json\"\n            },\n            body: JSON.stringify({\n                username,\n                password\n            })\n        }).then(response=> response.json()).then(data=> {\n                if(data.error) {\n                    M.toast({html:data.error})\n                } else {\n                    localStorage.setItem(\"jwt\", data.token)\n                    localStorage.setItem(\"user\", JSON.stringify(data.user))\n\n                    dispatch({type:\"USER\", payload:data.user})\n                    M.toast({html:\"Logging you in!\"})\n                    history.push(\"/main\")\n                }\n\n\n            }).catch(err=> {\n                console.log(err)\n            })\n    }\n\n    return(\n        <div >\n            <div class=\"row mycard auth-card\">\n                <h1>Sign up</h1>\n                <div class=\"col s12 m5 auth-card \">\n                    <div class=\"card-panel #424242 grey hoverable darken-3\">\n                        <input value={username} onChange={(e)=>setUsername(e.target.value)} className=\"white-text hoverable\" type=\"text\" placeholder=\"Username\"/>\n                        <input value={password} onChange={(e)=>setPassword(e.target.value)} className=\"white-text hoverable\" type=\"text\" placeholder=\"Password\"/>\n                        <button onClick={()=> {PostData()}} className=\"btn waves-effect waves-light #757575 grey darken-1\" type=\"submit\" name=\"action\">Log in\n                            <i className=\"material-icons right \">send</i>\n                        </button>\n                    </div>\n                </div>\n                <p>Don't have an account? Sign up <Link to=\"/signup\">here</Link></p>\n            </div>\n        </div>\n    )\n}\n\nexport default Signin","/Users/dhoolie/programs/BTCWallet/frontend/src/components/screens/signup.js",["62","63","64","65"],"import React, {useState, useEffect} from 'react'\nimport {Link, useHistory} from 'react-router-dom'\nimport M from 'materialize-css'\n\nconst Signup = () => {\n    const [username,setUsername] = useState(\"\")\n    const [password,setpassword] = useState(\"\")\n    const [spassword,setspassword] = useState(\"\")\n    const [email,setemail] = useState(\"\")\n    const history = useHistory()\n\n    const Upload = ()=> {\n\n        if(!/^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/.test(email)){\n            M.toast({html: \"invalid email\"})\n            return\n        }\n\n        if (spassword != password) {\n            M.toast({html: \"Passwords do not match\"})\n            return\n        }\n\n        fetch(\"/signup\", {\n            method: \"POST\",\n            headers: {\n                \"Content-Type\":\"application/json\"\n            },\n            body: JSON.stringify({\n                username, \n                email,\n                password\n            })\n        }).then(res=> res.json()).then(data=> {\n            if(data.error) {\n                M.toast({html:data.error})\n            } else {\n                M.toast({html:\"You have been signed up! Check your email!\"})\n                history.push(\"/main\")\n            }\n        }).catch(err=> {\n            console.log(err)\n        })\n    }\n\n    return(\n        <div >\n            <div class=\"row mycard auth-card\">\n                <h1>Sign up</h1>\n                <div class=\"col s12 m5 auth-card \">\n                    <div class=\"card-panel #424242 grey hoverable darken-3\">\n                        <input value={username} onChange={(e)=>setUsername(e.target.value)} className=\"white-text hoverable\" type=\"text\" placeholder=\"Username\"/>\n                        <input value={email} onChange={(e)=>setemail(e.target.value)} className=\"white-text hoverable\" type=\"text\" placeholder=\"Email\"/>\n                        <input value={password} onChange={(e)=>setpassword(e.target.value)} className=\"white-text hoverable\" type=\"text\" placeholder=\"Password\"/>\n                        <input value={spassword} onChange={(e)=>setspassword(e.target.value)}  className=\"white-text hoverable\" type=\"text\" placeholder=\"Re-enter password\"/>\n                        <button onClick={()=>Upload()} className=\"btn waves-effect waves-light #757575 grey darken-1\" type=\"submit\" name=\"action\">Sign up\n                            <i className=\"material-icons right \">send</i>\n                        </button>\n                    </div>\n                </div>\n                <p>Already have an account? Sign in <Link to=\"/signin\">here</Link></p>\n            </div>\n        </div>\n    )\n}\n\nexport default Signup","/Users/dhoolie/programs/BTCWallet/frontend/src/components/screens/about.js",["66"],"\nimport React from 'react'\nimport {Link} from 'react-router-dom'\n\n\n\n\n\nconst About = () => {\n\n\n    return (\n        <p>This is the about page</p>\n    )\n\n\n}\n\n\n\n\n\nexport default About","/Users/dhoolie/programs/BTCWallet/frontend/src/components/screens/mainpage.js",["67","68","69","70","71","72","73","74"],"/Users/dhoolie/programs/BTCWallet/frontend/src/components/screens/transaction.js",[],"/Users/dhoolie/programs/BTCWallet/frontend/src/context.js",["75","76"],"import { create } from 'qrcode'\nimport React, {createContext} from 'react'\n\n\nexport const usercontext = createContext()","/Users/dhoolie/programs/BTCWallet/f